package gc;

public class PrintGCTest {
    public static void main(String[] args) {
        byte[] b = null;
        long s = System.currentTimeMillis();
        for (int i = 0; i < 10; i++) {
            b = new byte[1 * 1024 * 1024];
        }
        System.out.println(System.currentTimeMillis()-s);
    }
}
