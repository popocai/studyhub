<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<context-param>
		<param-name>contextConfigLocation</param-name>
		<param-value>classpath*:/application-context.xml</param-value>
	</context-param>
	<servlet>
		<servlet-name>TestTime</servlet-name>
		<display-name>TestTime</display-name>
		<description></description>
		<servlet-class>org.patrick.learn.ignite.TestTime</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>SqlQuery</servlet-name>
		<display-name>SqlQuery</display-name>
		<description></description>
		<servlet-class>org.patrick.learn.ignite.sql.SqlQuery</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>TestTime</servlet-name>
		<url-pattern>/tt</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>SqlQuery</servlet-name>
		<url-pattern>/SqlQuery</url-pattern>
	</servlet-mapping>

	

</web-app>
